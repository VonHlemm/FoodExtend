{
  "manifest_version": 2,

  "name": "Food Manifest",
  "description": "This extension helps you to document your gulping during the day.",
  "version": "1.0",

   "permissions": [
    "activeTab",
    "widgets"
  ],

    "web_accessible_resources": [
    "icon.png"
  ],

  "background": {
    "scripts": ["food.js"]
  }

  "browser_action": {
    "default_title": "Food-Manifest"
  }
}
